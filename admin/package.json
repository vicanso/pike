{
  "name": "spt-admin",
  "version": "0.1.0",
  "scripts": {
    "format": "node node_modules/.bin/prettier --write *.js */*.js */*.vue */*/*.js */**/*.vue",
    "serve": "node node_modules/.bin/vue-cli-service serve --open",
    "build": "NODE_ENV=production node node_modules/.bin/vue-cli-service build",
    "lint": "yarn format && node node_modules/.bin/vue-cli-service lint"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "dayjs": "^1.5.17",
    "element-ui": "^2.2.2",
    "json-diff": "^0.5.2",
    "lodash": "^4.17.5",
    "node-sass": "^4.8.1",
    "prettier": "^1.11.1",
    "pug": "^2.0.3",
    "sass-loader": "^6.0.7",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "ENV": true,
      "APP": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
